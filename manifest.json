{
  "manifest_version": 3,
  "name": "CS61A Dark Theme",
  "version": "1.1.5",
  "description": "Dark Mode designed for cs61a.org (only)",
  "content_scripts": [
    {
      "matches": [
        "https://cs61a.org/*",
        "http://cs61a.org/*"
      ],
      "css": [
        "dark.css"
      ],
      "run_at": "document_start"
    }
  ]
}
